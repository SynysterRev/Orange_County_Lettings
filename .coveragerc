[run]
source =
    lettings
    profiles
    oc_lettings_site

omit =
    */migrations/*
    */tests/*
    */settings/*
    */apps.py
    manage.py
    conftest.py
    */__init__.py
    */asgi.py
    */wsgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError


